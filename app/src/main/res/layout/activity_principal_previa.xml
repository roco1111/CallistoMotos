<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/background"

    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margin_top_menu"
        android:id="@+id/id_superior"
        android:orientation="horizontal">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/id_turno"
        android:orientation="vertical">
    <ImageButton
        android:id="@+id/imageButtonTurno"
        android:layout_width="@dimen/ancho_boton"
        android:layout_height="@dimen/ancho_boton"

        android:layout_centerHorizontal="true"

        android:background="@color/background"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/turno" />
    <TextView
        android:id="@+id/turno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButtonTurno"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margin_top_texto"
        android:text="@string/nuevo_turno"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="@dimen/tamanio_parrafo" />
    </RelativeLayout>
        <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/id_historial"
        android:layout_toEndOf="@id/id_turno"
        android:layout_marginStart="@dimen/margin_top_menu"
        android:orientation="vertical">
    <ImageButton
        android:id="@+id/imageButtonHistorial"
        android:layout_width="@dimen/ancho_boton"
        android:layout_height="@dimen/ancho_boton"
        android:background="@color/background"
        android:layout_centerHorizontal="true"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/historial" />
    <TextView
        android:id="@+id/historial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButtonHistorial"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margin_top_texto"
        android:text="@string/historial_viaje"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="@dimen/tamanio_parrafo" />
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/id_superior"
        android:layout_marginTop="@dimen/margin_top_menu"
        android:id="@+id/id_inferior"
        android:orientation="horizontal">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/id_perfil"
        android:orientation="vertical">
    <ImageButton
        android:id="@+id/imageButtonPerfil"
        android:layout_width="@dimen/ancho_boton"
        android:layout_height="@dimen/ancho_boton"
        android:background="@color/background"
        android:layout_centerHorizontal="true"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/perfil" />

    <TextView
        android:id="@+id/perfil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButtonPerfil"
        android:layout_centerHorizontal="true"
        android:text="@string/menu_preferencias"
        android:layout_marginTop="@dimen/margin_top_texto"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="@dimen/tamanio_parrafo" />
    </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/id_impresora"
            android:layout_toEndOf="@id/id_perfil"
            android:layout_marginStart="@dimen/alto_imagen"
            android:orientation="vertical">
            <ImageButton
                android:id="@+id/imageButtonImpresora"
                android:layout_width="@dimen/ancho_boton"
                android:layout_height="@dimen/ancho_boton"
                android:background="@color/background"
                android:layout_centerHorizontal="true"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/ic_impresora" />
            <TextView
                android:id="@+id/impresora"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonImpresora"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/margin_top_texto"
                android:text="@string/impresora"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="@dimen/tamanio_parrafo" />
        </RelativeLayout>
    </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_inferior"
            android:id="@+id/id_recaudacion"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/margin_top_menu"
            android:orientation="vertical">
            <ImageButton
                android:id="@+id/imageButtonRecaudacion"
                android:layout_width="@dimen/ancho_boton"
                android:layout_height="@dimen/ancho_boton"
                android:background="@color/background"
                android:layout_centerHorizontal="true"

                android:scaleType="fitXY"
                app:srcCompat="@drawable/recaudacion" />
            <TextView
                android:id="@+id/recaudacion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageButtonRecaudacion"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/margin_top_texto"
                android:text="@string/recaudacion"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="@dimen/tamanio_parrafo" />

        </RelativeLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/id_recaudacion"
        android:layout_marginTop="@dimen/margin_top_menu"
        android:id="@+id/id_teclado_sup"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButtonNueve"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/nueve" />
        <ImageButton
            android:id="@+id/imageButtonOcho"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:layout_toEndOf = "@id/imageButtonNueve"
            android:scaleType="fitXY"
            android:layout_marginStart="@dimen/campos_height"
            app:srcCompat="@drawable/ocho" />

        <ImageButton
            android:id="@+id/imageButtonSiete"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:layout_marginStart="@dimen/campos_height"
            android:layout_toEndOf = "@id/imageButtonOcho"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/siete" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/id_teclado_sup"
        android:layout_marginTop="@dimen/margin_top_menu2"
        android:id="@+id/id_teclado_medio"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButtonSeis"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/seis" />
        <ImageButton
            android:id="@+id/imageButtonCinco"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:layout_toEndOf = "@id/imageButtonSeis"
            android:scaleType="fitXY"
            android:layout_marginStart="@dimen/campos_height"
            app:srcCompat="@drawable/cinco" />

        <ImageButton
            android:id="@+id/imageButtonCuatro"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:layout_marginStart="@dimen/campos_height"
            android:layout_toEndOf = "@id/imageButtonCinco"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/cuatro" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/id_teclado_medio"
        android:layout_marginTop="@dimen/margin_top_menu2"
        android:id="@+id/id_teclado_inferior"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButtonTres"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/tres" />
        <ImageButton
            android:id="@+id/imageButtonDos"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:layout_toEndOf = "@id/imageButtonTres"
            android:scaleType="fitXY"
            android:layout_marginStart="@dimen/campos_height"
            app:srcCompat="@drawable/dos" />

        <ImageButton
            android:id="@+id/imageButtonUno"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:layout_marginStart="@dimen/campos_height"
            android:layout_toEndOf = "@id/imageButtonDos"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/uno" />

        <ImageButton
            android:id="@+id/imageButtonCero"
            android:layout_width="@dimen/ancho_boton"
            android:layout_height="@dimen/ancho_boton"
            android:background="@color/background"
            android:layout_marginStart="@dimen/campos_height"
            android:layout_toEndOf = "@id/imageButtonUno"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/cero" />


    </RelativeLayout>

</RelativeLayout>